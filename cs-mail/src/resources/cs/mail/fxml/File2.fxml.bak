<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="549.0" prefWidth="791.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.cs.mail.controller.file.FileController">
   <children>
      <HBox spacing="5.0" style="-fx-border-width: 0.5; -fx-border-color: gray;">
         <children>
            <TextField minWidth="500.0" />
            <Button mnemonicParsing="false" text="검색" />
            <ComboBox promptText="검색조건" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <TreeView fx:id="folderTree" focusTraversable="false" maxWidth="250.0" minWidth="250.0" prefHeight="526.0" prefWidth="250.0" />
            <TableView fx:id="fileTableView" focusTraversable="false" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="firstColumn" prefWidth="75.0" text="파일" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <TabPane fx:id="fileView" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
